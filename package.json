{
	"name": "todolist",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"preview": "vite preview",
		"generate:api": "npx swagger-typescript-api -p $(grep VITE_APP_API_URL .env | cut -d '=' -f2)/swagger/doc.json -o ./src/app/types/api --no-client --modular --responses --extract-request-params --extract-request-body"
	},
	"dependencies": {
		"@vueuse/core": "^11.1.0",
		"axios": "^1.7.7",
		"jwt-decode": "^4.0.0",
		"pinia": "^2.2.4",
		"pinia-plugin-persistedstate": "^3.2.1",
		"vue": "^3.5.12",
		"vue-router": "^4.4.5",
		"vuetify": "^3.7.3"
	},
	"devDependencies": {
		"@mdi/font": "^7.4.47",
		"@mdi/js": "^7.4.47",
		"@types/node": "^22.7.2",
		"@typescript-eslint/eslint-plugin": "6.10.0",
		"@typescript-eslint/parser": "6.10.0",
		"@vitejs/plugin-vue": "^5.1.2",
		"eslint": "8.53.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-simple-import-sort": "10.0.0",
		"eslint-plugin-vue": "9.18.1",
		"prettier": "3.0.3",
		"sass": "1.69.5",
		"sass-embedded": "^1.79.1",
		"swagger-typescript-api": "^13.0.22",
		"typescript": "^5.2.2",
		"unplugin-auto-import": "^0.17.6",
		"vite": "^5.4.1",
		"vite-plugin-checker": "0.6.2",
		"vue-tsc": "^1.8.22"
	}
}
